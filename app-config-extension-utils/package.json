    "fix": "eslint --fix src",
    "test": "jest",
    "prepublishOnly": "yarn clean && yarn build && yarn build:es"
  },
  "dependencies": {
    "@app-config/core": "^2.9.0-beta.1",
    "@serafin/schema-builder": "0.14"
  },
  "devDependencies": {},
  "prettier": "@lcdev/prettier",
  "jest": {
    "preset": "@lcdev/jest"
  }
}
